Cell[BoxData[{
 RowBox[{
  RowBox[{"getStatesGraph", "[", 
   RowBox[{"caRules_", ",", "rowSize_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"ResourceFunction", "[", "\"\<MultiwaySystem\>\"", "]"}], "[", 
   RowBox[{
    RowBox[{"CellularAutomaton", "[", "caRules", "]"}], ",", "\n", "\t", 
    RowBox[{"Tuples", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"0", ",", "1"}], "}"}], ",", "rowSize"}], "]"}], ",", "1", 
    ",", "\"\<StatesGraph\>\"", ",", 
    RowBox[{"ImageSize", "\[Rule]", "Medium"}], ",", "\n", "\t", 
    RowBox[{"\"\<StateRenderingFunction\>\"", "->", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Inset", "[", 
        RowBox[{
         RowBox[{"ArrayPlot", "[", 
          RowBox[{
           RowBox[{"{", "#2", "}"}], ",", 
           RowBox[{"ImageSize", "\[Rule]", "30"}]}], "]"}], ",", "#1", ",", 
         "#3"}], "]"}], "&"}], ")"}]}]}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"confluentQ", "[", "graph_", "]"}], ":=", 
  RowBox[{"AllTrue", "[", 
   RowBox[{
    RowBox[{"WeaklyConnectedGraphComponents", "[", "graph", "]"}], ",", "\n", 
    "\t", 
    RowBox[{"Function", "[", 
     RowBox[{"cc", ",", 
      RowBox[{"AnyTrue", "[", 
       RowBox[{
        RowBox[{"VertexList", "[", "cc", "]"}], ",", 
        RowBox[{
         RowBox[{"ContainsExactly", "[", 
          RowBox[{
           RowBox[{"VertexInComponent", "[", 
            RowBox[{"cc", ",", "#"}], "]"}], ",", 
           RowBox[{"VertexList", "[", "cc", "]"}]}], "]"}], "&"}]}], "]"}]}], 
     "]"}]}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"confluentStates", "[", "graph_", "]"}], ":=", 
  RowBox[{
   RowBox[{"Function", "[", 
    RowBox[{"cc", ",", 
     RowBox[{"Select", "[", 
      RowBox[{
       RowBox[{"VertexList", "[", "cc", "]"}], ",", "\n", "\t", 
       RowBox[{
        RowBox[{"ContainsExactly", "[", 
         RowBox[{
          RowBox[{"VertexInComponent", "[", 
           RowBox[{"cc", ",", "#"}], "]"}], ",", 
          RowBox[{"VertexList", "[", "cc", "]"}]}], "]"}], "&"}]}], "]"}]}], 
    "]"}], "/@", 
   RowBox[{"WeaklyConnectedGraphComponents", "[", "graph", "]"}]}]}], "\n", 
 RowBox[{
  RowBox[{"totalConfluentQ", "[", "graph_", "]"}], ":=", 
  RowBox[{"CompleteGraphQ", "[", 
   RowBox[{"TransitiveClosureGraph", "[", "graph", "]"}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{"getSystemInfo", "[", 
   RowBox[{"caRules_", ",", "rowSize_"}], "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"statesGraph", "=", 
      RowBox[{"getStatesGraph", "[", 
       RowBox[{"caRules", ",", "rowSize"}], "]"}]}], "}"}], ",", 
    RowBox[{"<|", "\n", "\t", 
     RowBox[{
      RowBox[{"\"\<rules\>\"", "\[Rule]", "caRules"}], ",", 
      RowBox[{"\"\<rowSize\>\"", "\[Rule]", "rowSize"}], ",", "\n", "\t", 
      RowBox[{"\"\<statesGraph\>\"", "\[Rule]", "statesGraph"}], ",", "\n", 
      "\t", 
      RowBox[{"\"\<confluentQ\>\"", "\[Rule]", 
       RowBox[{"confluentQ", "[", "statesGraph", "]"}]}], ",", "\n", "\t", 
      RowBox[{"\"\<totalConfluentQ\>\"", "\[Rule]", 
       RowBox[{"totalConfluentQ", "[", "statesGraph", "]"}]}], ",", "\n", 
      "\t", 
      RowBox[{"\"\<confluentStates\>\"", "\[Rule]", 
       RowBox[{"Length", "[", 
        RowBox[{"Flatten", "[", 
         RowBox[{
          RowBox[{"confluentStates", "@", "statesGraph"}], ",", "1"}], "]"}], 
        "]"}]}]}], "\n", "\t", "|>"}]}], "]"}]}]}], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"d6d190fb-e924-415d-a4f8-222b965e45a1"],

